{
  "name": "decora-browser-lib-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build.lib": "ng build @decora/browser-lib-ui && npm run copy-bootstrap && npm run copy-bootstrap && npm run copy-themes && npm run copy-lib-readmes && npm run copy-bootstrap-readmes",
    "copy-bootstrap": "cpx \"./projects/decora/browser-lib-ui/bootstrap/**/*\" \"./dist/decora/browser-lib-ui/bootstrap/\"",
    "copy-themes": "cpx \"./projects/decora/browser-lib-ui/src/lib/**/*theming.scss\" \"./dist/decora/browser-lib-ui/bootstrap/theming/\"",
    "copy-lib-readmes": "cpx \"./projects/decora/browser-lib-ui/src/lib/**/*.md\" \"./dist/decora/browser-lib-ui/lib/\"",
    "copy-bootstrap-readmes": "cpx \"./projects/decora/browser-lib-ui/bootstrap/**/*.md\" \"./dist/decora/browser-lib-ui/bootstrap/\"",
    "mock.server": "nodemon ./../decora-mock-server/mock-server.js",
    "start.mock": "concurrently --kill-others \"npm run mock.server\" \"npm run start\"",
    "start.mock-aot": "concurrently --kill-others \"npm run mock.server\" \"ng s --aot\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.3.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.3.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngu/carousel": "^1.4.8",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@swimlane/ngx-datatable": "^13.0.1",
    "angular2-prettyjson": "^3.0.1",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ngx-image-zoom": "^0.3.1",
    "ngx-markdown": "^6.0.1",
    "prismjs": "^1.15.0",
    "roboto-fontface": "^0.9.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.14.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "nodemon": "^1.17.5",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "underscore": "^1.9.1"
  }
}
